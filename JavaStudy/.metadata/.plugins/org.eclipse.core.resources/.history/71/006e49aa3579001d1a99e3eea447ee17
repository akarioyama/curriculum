/**
 *
 */
package interfaces;

/**
 * @author Akari_OYAMA
 *
 */
public abstract class PolimoSuper {
	protected void call() {
		System.out.println(this.getClass());
	}
}
	/**
	 *
	 */

class PolimoSub1 extends PolimoSuper{
	@Override
	protected void call() {
		super.call();
	}
}

class PolimoSub2 extends PolimoSuper{
	@Override
	protected void call() {
		super.call();
	}
}

PolimoSuper polimo1 = new PolimoSub1();
polimo1.call();

PolimoSuper polim2 = new PolimoSub2();
polimo2.call();
