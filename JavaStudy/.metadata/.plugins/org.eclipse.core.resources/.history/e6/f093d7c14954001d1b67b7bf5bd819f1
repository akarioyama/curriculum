    public static String getName(String firstName, String lastName) {
        System.out.println(getName(firstName, lastName));
        System.out.println();
        return "firstName" + "lastName" ;
    }

    public void isOdd(int num) {
        if (num % 2 != 0) {
            System.out.println(num + "は奇数です。");
        }
    }
}