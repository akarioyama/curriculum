package check;

import check.Pet;
import check.RobotPet;
import constants.Constants;

@SuppressWarnings("unused")
public class Check {
	public static void  main(String[] args) {

		String  firstName = "akari" ;
		String  lastName = "oyama";
		System.out.println("printNameメソッド→" + printName(firstName,lastName));


		Pet pt = new Pet(firstName, lastName);
		pt.introduce();
		System.out.println();
		RobotPet rpt = new RobotPet(firstName, lastName);
		rpt.introduce();
		System.out.println();

	}


	public void pt(String getname, String getMasterName, String introduce) {
		System.out.println();
	}

	public  void rpt(String getName , String getMasterName ) {
		System.out.println();
	}

		private static String  printName(String firstName , String lastName) {
			return firstName + lastName ;
		}
}